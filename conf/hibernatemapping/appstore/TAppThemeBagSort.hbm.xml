<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.energysh.egame.po.appstore.TAppThemeBagSort" table="t_app_theme_bag_sort">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="themeBagId" type="java.lang.Integer">
			<column name="theme_bag_id" />
		</property>
		<property name="sort" type="java.lang.Integer">
			<column name="sort" />
		</property>
		<property name="ctime" type="timestamp">
			<column name="ctime" />
		</property>
	</class>
</hibernate-mapping>
