<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.energysh.egame.po.appstore.TAppCategory" table="t_app_category">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="fatherId" type="java.lang.Integer">
			<column name="father_id" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="name" length="50" />
		</property>
		<property name="seq" type="java.lang.Integer">
			<column name="seq" />
		</property>
		<property name="icon" type="java.lang.String">
			<column name="icon" length="100" />
		</property>
		<property name="remark" type="java.lang.String">
			<column name="remark" length="500" />
		</property>
		<property name="level" type="java.lang.Integer">
			<column name="level" />
		</property>
		<property name="isOpex" type="java.lang.Integer">
			<column name="is_opex" />
		</property>
		<property name="pubStatus" type="java.lang.Integer">
			<column name="pub_status" />
		</property>
		<property name="ctime" type="timestamp">
			<column name="ctime" />
		</property>
		<property name="uptime" type="timestamp">
			<column name="uptime" />
		</property>
	</class>
</hibernate-mapping>
