<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.energysh.egame.po.appstore.TAppUserAccount" table="t_app_user_account">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="userId" type="java.lang.Integer">
			<column name="user_id" />
		</property>
		<property name="userName" type="java.lang.String">
			<column name="user_name" length="50" />
		</property>
		<property name="appId" type="java.lang.Integer">
			<column name="app_id" />
		</property>
		<property name="batchId" type="java.lang.String">
			<column name="batch_id" length="50" />
		</property>
		<property name="regType" type="java.lang.String">
			<column name="reg_type" length="20" />
		</property>
		<property name="osType" type="java.lang.String">
			<column name="os_type" length="10" />
		</property>
		<property name="ip" type="java.lang.String">
			<column name="ip" length="20" />
		</property>
		<property name="ctime" type="timestamp">
			<column name="ctime" />
		</property>
	</class>
</hibernate-mapping>
