<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.energysh.egame.po.appstore.TSaleBatchRelation" table="t_sale_batch_relation">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="userId" type="java.lang.Integer">
			<column name="user_id" />
		</property>
		<property name="batchId" type="java.lang.String">
			<column name="batch_id" />
		</property>
		<property name="divideRate" type="java.lang.String">
			<column name="divide_rate" />
		</property>
		<property name="del" type="java.lang.Integer">
			<column name="del" />
		</property>
		<property name="ctime" type="timestamp">
			<column name="ctime" />
		</property>
	</class>
</hibernate-mapping>
