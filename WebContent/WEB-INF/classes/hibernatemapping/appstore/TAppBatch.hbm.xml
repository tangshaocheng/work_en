<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.energysh.egame.po.appstore.TAppBatch" table="t_app_batch">
		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="appId" type="java.lang.Integer">
			<column name="app_id" />
		</property>
		<property name="batchId" type="java.lang.String">
			<column name="batch_id" />
		</property>
		<property name="app" type="java.lang.String">
			<column name="app" length="500" />
		</property>
		<property name="appSize" type="java.lang.Double">
			<column name="app_size" />
		</property>
		<property name="ctime" type="timestamp">
			<column name="ctime" />
		</property>
	</class>
</hibernate-mapping>
